	"cmd-id-pattern": "$_RPC_CMD",
	"evt-id-pattern": "$_RPC_EVT",
	"indent-size": 8,
	"indent-string": "\t",
	"generator": "NrfRpcCborGenerator",
	"clang-params": "--config ${include-stubs}/conf.txt",
	"clang-cli-params": "-DCONFIG_BT_RPC_CLIENT=1",
	"clang-host-params": "-DCONFIG_BT_RPC_HOST=1",
	"include-stubs": {
		"conf.txt": [
			"-include _rpc_defines.h",
			"-Wno-int-to-pointer-cast",
			"-Wno-undefined-internal",
			"-I${include-stubs}",
			"-Iclient/include",
			"-Ihost/include",
			"-Icommon/include",
			"-I../../nrf_rpc/include",
			"-I../../../../nrfxlib/nrf_rpc/include",
			"-I../../../../modules/lib/tinycbor/include",
			"-I../../../../zephyr/include",
			"-I../../../../modules/lib/open-amp/open-amp/lib/include",
			"-I../../../../nrf/samples/nrf_rpc/entropy_nrf53/cpuapp/build_nrf5340pdk_nrf5340_cpuapp/modules/libmetal/libmetal/lib/include"
		],
		"_rpc_defines.h": [
			"#define __GENERATOR",
